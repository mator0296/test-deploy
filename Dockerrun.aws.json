{
  "AWSEBDockerrunVersion": 2,

  "containerDefinitions": [
    {
      "command": [
        "python",
        "-m",
        "celery",
        "-A",
        "mesada",
        "flower",
        "-B",
        "--loglevel=DEBUG"
      ],
      "essential": true,
      "image": "322086058235.dkr.ecr.us-east-1.amazonaws.com/graphql-server-django",
      "links": ["redis", "celery"],
      "memory": 128,
      "name": "flower",
      "portMappings": [
        {
          "containerPort": 5555,
          "hostPort": 5555
        }
      ]
    },
    {
      "command": [
        "python",
        "-m",
        "celery",
        "-A",
        "mesada",
        "beat",
        "--loglevel=DEBUG"
      ],
      "essential": true,
      "image": "322086058235.dkr.ecr.us-east-1.amazonaws.com/graphql-server-django",
      "links": ["redis", "celery"],
      "memory": 128,
      "name": "beat"
    },
    {
      "command": [
        "python",
        "-m",
        "celery",
        "-A",
        "mesada",
        "worker",
        "-B",
        "--loglevel=DEBUG"
      ],
      "essential": true,
      "image": "322086058235.dkr.ecr.us-east-1.amazonaws.com/graphql-server-django",
      "links": ["redis"],
      "memory": 128,
      "name": "celery"
    },
    {
      "essential": true,
      "image": "322086058235.dkr.ecr.us-east-1.amazonaws.com/graphql-server-django",
      "links": ["redis"],
      "memory": 128,
      "name": "django",
      "portMappings": [
        {
          "containerPort": 8000,
          "hostPort": 80
        }
      ]
    },

    {
      "essential": true,
      "image": "redis:latest",
      "memory": 128,
      "name": "redis",
      "portMappings": [
        {
          "containerPort": 6379,
          "hostPort": 6379
        }
      ]
    }
  ],
  "family": "",
  "volumes": []
}
