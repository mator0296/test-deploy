{
    "containerDefinitions": [
        {
            "command": [
                "celery",
                "-A",
                "mesada",
                "worker",
                "-B",
                "--loglevel=DEBUG"
            ],
            "essential": true,
            "image": "python",
            "links": [
                "redis"
            ],
            "memory": 1000,
            "name": "celery"
        },
        {
            "essential": true,
            "image": "python",
            "links": [
                "redis"
            ],
            "memory": 1000,
            "name": "django",
            "portMappings": [
                {
                    "containerPort": 8000,
                    "hostPort": 8000
                }
            ]
        },
        {
            "essential": true,
            "image": "redis:latest",
            "memory": 1000,
            "name": "redis",
            "portMappings": [
                {
                    "containerPort": 6379,
                    "hostPort": 6379
                }
            ]
        }
    ],
    "family": "",
    "volumes": []
}
