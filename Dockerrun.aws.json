{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "command": [
        "celery",
        "-A",
        "dockerexample",
        "worker",
        "-B",
        "--loglevel=DEBUG"
      ],
      "essential": true,
      "image": "python",
      "links": ["redis"],
      "memory": 128,
      "mountPoints": [
        {
          "containerPath": "/var/www/djangoproject",
          "sourceVolume": "Dockerexample"
        }
      ],
      "name": "celery"
    },
    {
      "essential": true,
      "image": "python",
      "links": ["redis"],
      "memory": 128,
      "mountPoints": [
        {
          "containerPath": "/var/www/djangoproject",
          "sourceVolume": "Dockerexample"
        }
      ],
      "name": "django",
      "portMappings": [
        {
          "containerPort": 8000,
          "hostPort": 8000
        }
      ]
    },
    {
      "essential": true,
      "image": "redis:latest",
      "memory": 128,
      "name": "redis",
      "portMappings": [
        {
          "containerPort": 6379,
          "hostPort": 6379
        }
      ]
    }
  ],
  "family": "",
  "volumes": [
    {
      "host": {
        "sourcePath": "./dockerexample"
      },
      "name": "Dockerexample"
    }
  ]
}
